{
  "expo": {
    "name": "Gestab",
    "slug": "gestab",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo/gestab.webp",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo/gestab.webp",
        "backgroundColor": "#ffffff"
      },
      "package": "com.marlof2.gestab"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "51d40b76-d08c-4f47-8f49-194a7a8cf2e6"
      }
    },
    "owner": "marlof2"
  }
}
