{
  "expo": {
    "name": "Gestab",
    "slug": "gestab",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo/gestab.jpg",
    "scheme": "com.marlof2.gestab",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.marlof2.gestab",
      "config": {
        "usesNonExemptEncryption": false
      },
      "associatedDomains": ["applinks:orange-manatee-370597.hostingersite.com"]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo/gestab.jpg",
        "backgroundColor": "#ffffff"
      },
      "versionCode": 1,
      "package": "com.marlof2.gestab",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "com.marlof2.gestab"
            },
            {
              "scheme": "https",
              "host": "orange-manatee-370597.hostingersite.com",
              "pathPrefix": "/api/google/callback"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-6385061559848945~7033838940",
          "iosAppId": "ca-app-pub-xxxxxxxx~xxxxxxxx"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "944125af-38a9-410d-9e61-82f6365d1193"
      }
    },
    "owner": "marlof2"
  }
}
