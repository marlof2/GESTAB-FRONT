{
  "expo": {
    "name": "GestAB",
    "slug": "gestab",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo/gestab.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.marlof2.gestab"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.marlof2.gestab"
    },
    "scheme": "com.marlof2.gestab",
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-6385061559848945~7033838940",
          "iosAppId": "ca-app-pub-xxxxxxxx~xxxxxxxx"
        }
      ]
    ],
    "linking": {
      "enabled": true,
      "prefixes": [
        "com.marlof2.gestab://",
        "https://orange-manatee-370597.hostingersite.com/api/google"
      ],
      "config": {
        "screens": {
          "SignIn": "SignIn",
          "SelectEstablishment": "SelectEstablishment",
          "CompleteProfile": "CompleteProfile"
        }
      }
    },
    "extra": {
      "eas": {
        "projectId": "944125af-38a9-410d-9e61-82f6365d1193"
      }
    }
  }
}
